{
"manifest_version" : 2,
"name" : "Belka VPN",
"description" : "The BelkaVPN extension is a remote control for the desktop app",
"version" : "1.0.0",
"browser_action":
{
    "default_icon":"./img/ic_launcher_round.png",
    "default_popup" : "vs.html"
},
"content_security_policy": "script-src 'self' https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css ; object-src 'self'",

"permissions":["activeTab"]
}